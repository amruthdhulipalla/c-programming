#include<stdio.h>
#include<math.h>
int main(){
    int A,B,C;
    printf("Enter the length of A ,B ,C :");
    scanf("%d %d %d",&A,&B,&C);
    if (A==B && B==C)
    {
        printf("it's an equilateral triangle:");
        
    }
    else if(A==B||B==C||C==A){
        printf("it's an isosceles triangle");
    }
    else {
        printf("it's an scalene triangle");
    }
}
